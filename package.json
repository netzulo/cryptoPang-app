{
  "name": "my-map-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "start": "vite preview -p 80",
    "css": "npx tailwindcss -i \"src/assets/css/input.css\" -o \"public/output.css\"",
    "css:dev": "npx tailwindcss -i \"./src/assets/css/input.css\" -o \"public/output.css\" --watch",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,}\" \".storybook/stories/**/*.{js,jsx,ts,tsx,}\" \"__tests__/**/*test*.{js,jsx,ts,tsx,}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,}\" \".storybook/stories/**/*.{js,jsx,ts,tsx,}\" \"__tests__/**/*test*.{js,jsx,ts,tsx,}\" --config \".prettier.json\"",
    "test": "jest --collectCoverage --coverageDirectory=\".coverage/\" --ci --reporters=default --reporters=jest-junit --watchAll=false --detectOpenHandles --forceExit",
    "build": "tsc -b && vite build",
    "ci": "npm run css && npm run lint && npm run format && npm run test && npm run build",
    "clean": "rm -rf dist dist-stories dist-docs .coverage package-lock.json junit.xml yarn.lock node_modules"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@headlessui/tailwindcss": "^0.2.0",
    "@heroicons/react": "^2.2.0",
    "@react-three/cannon": "^6.6.0",
    "@react-three/drei": "^9.120.4",
    "@react-three/fiber": "^8.17.10",
    "animate.css": "^4.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.2",
    "three": "^0.171.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@eslint/js": "^9.15.0",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/testing-library__react": "^10.2.0",
    "@types/three": "^0.171.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^29.7.0",
    "daisyui": "^4.12.22",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-storybook": "^0.11.1",
    "eslint-plugin-testing-library": "^7.1.1",
    "globals": "^15.12.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-svg-transformer": "^1.0.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "storybook": "^8.4.7",
    "tailwindcss": "^3.4.16",
    "tailwindcss-children": "^2.1.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "^.+\\.svg$": "jest-svg-transformer",
      "^.+\\.(css|less|scss)$": "identity-obj-proxy",
      "^uuid$": "<rootDir>/node_modules/uuid/dist/index.js",
      "^@azure/msal-react": "<rootDir>/node_modules/@azure/msal-react/dist/index.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "coverageReporters": [
      "html",
      "text",
      "text-summary",
      "cobertura"
    ]
  }
}
